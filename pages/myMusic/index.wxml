<view>
	<view class="weui-cells weui-cells_after-title">
		<navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
			<view class="weui-cell__hd">
				<image src="/images/cm2_list_icn_recent_new@2x.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
			</view>
			<view class="weui-cell__bd">最近播放</view>
			<view class="weui-cell__ft weui-cell__ft_in-access">0</view>
		</navigator>
		<navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
			<view class="weui-cell__hd">
				<image src="/images/cm2_list_icn_rdi_new@2x.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
			</view>
			<view class="weui-cell__bd">我的电台</view>
			<view class="weui-cell__ft weui-cell__ft_in-access">0</view>
		</navigator>
		<navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
			<view class="weui-cell__hd">
				<image src="/images/cm4_my_icn_fav@2x.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
			</view>
			<view class="weui-cell__bd">我的收藏</view>
			<view class="weui-cell__ft weui-cell__ft_in-access">0</view>
		</navigator>
	</view>
	<view class="weui-panel weui-panel_access">
		<view class="weui-panel__hd">我创建的歌单({{listCount}})</view>
		<view class="weui-panel__bd">
			<navigator
			 url="/pages/likeList/likeList"
			 open-type="switchTab"
			 class="weui-media-box weui-media-box_appmsg"
			 hover-class="weui-cell_active"
			>
				<view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
					<image class="weui-media-box__thumb" src="/images/cm2_play_icn_loved@2x.png" />
				</view>
				<view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
					<view class="weui-media-box__title">我喜欢的音乐</view>
					<view class="weui-media-box__desc">{{likeList.length}}首</view>
				</view>
			</navigator>
		</view>
	</view>
</view>

