/* components/musicGrip/music-grip.wxss */
.weui-grid{
  padding:0;
}
.weui-grid>.image-container{
  width: 100%;
  height: 0px;
  padding-bottom: 100%;
  overflow:hidden;
  margin: 0;
  position:relative;
}

.weui-grid>.image-container>.img{
  position:absolute;
  width: 100%;
  height: 100%;
}

.music-title{
  white-space: unset;
  height: 100rpx;
  text-align: left;
}